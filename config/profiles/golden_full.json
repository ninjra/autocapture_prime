{
  "plugins": {
    "capabilities": {
      "answer.synthesizer": {
        "fanout": false,
        "max_providers": 1,
        "mode": "single",
        "preferred": [
          "builtin.answer.synth_vllm_localhost"
        ],
        "provider_ids": []
      },
      "embedder.text": {
        "fanout": false,
        "max_providers": 1,
        "mode": "single",
        "preferred": [
          "builtin.embedder.vllm_localhost"
        ],
        "provider_ids": []
      },
      "reranker": {
        "fanout": false,
        "max_providers": 1,
        "mode": "single",
        "preferred": [
          "builtin.reranker.colbert_hash",
          "builtin.reranker.basic"
        ],
        "provider_ids": []
      },
      "ocr.engine": {
        "fanout": false,
        "max_providers": 1,
        "mode": "single",
        "preferred": [
          "builtin.ocr.nemotron_torch",
          "builtin.ocr.rapid",
          "builtin.ocr.basic"
        ],
        "provider_ids": []
      },
      "processing.stage.hooks": {
        "fanout": true,
        "max_providers": 64,
        "mode": "multi",
        "preferred": [],
        "provider_ids": []
      },
      "state.vector_index": {
        "fanout": false,
        "max_providers": 1,
        "mode": "single",
        "preferred": [
          "builtin.state.vector.hnsw",
          "builtin.state.vector.sqlite_ts",
          "builtin.state.vector.linear"
        ],
        "provider_ids": []
      },
      "vision.extractor": {
        "fanout": true,
        "max_providers": 8,
        "mode": "multi",
        "preferred": [
          "builtin.vlm.vllm_localhost",
          "builtin.vlm.basic"
        ],
        "provider_ids": []
      }
    },
    "enabled": {
      "builtin.anchor.basic": true,
      "builtin.answer.synth_vllm_localhost": true,
      "builtin.capture.audio.windows": false,
      "builtin.capture.basic": false,
      "builtin.capture.screenshot.windows": false,
      "builtin.capture.windows": false,
      "builtin.embedder.basic": false,
      "builtin.embedder.vllm_localhost": true,
      "builtin.embeddings.basic": false,
      "builtin.index.colbert_hash": true,
      "builtin.journal.basic": true,
      "builtin.ledger.basic": true,
      "builtin.ocr.nemotron_torch": true,
      "builtin.ocr.rapid": true,
      "builtin.reranker.colbert_hash": true,
      "builtin.sst.build.delta": true,
      "builtin.sst.build.state": true,
      "builtin.sst.compliance.redact": true,
      "builtin.sst.extract.chart": true,
      "builtin.sst.extract.code": true,
      "builtin.sst.extract.spreadsheet": true,
      "builtin.sst.extract.table": true,
      "builtin.sst.index": true,
      "builtin.sst.infer.action": true,
      "builtin.sst.layout.assemble": true,
      "builtin.sst.match.ids": true,
      "builtin.sst.nemotron_objects": false,
      "builtin.sst.ocr.onnx": true,
      "builtin.sst.persist": true,
      "builtin.sst.preprocess.normalize": true,
      "builtin.sst.preprocess.tile": true,
      "builtin.sst.qa.answers": false,
      "builtin.sst.temporal.segment": true,
      "builtin.sst.track.cursor": true,
      "builtin.sst.ui.parse": true,
      "builtin.state.anomaly": true,
      "builtin.state.jepa.training": true,
      "builtin.state.workflow.miner": true,
      "builtin.tracking.clipboard.windows": false,
      "builtin.tracking.file_activity.windows": false,
      "builtin.tracking.input.windows": false,
      "builtin.window.metadata.windows": false
    },
    "permissions": {
      "localhost_allowed_plugin_ids": [
        "builtin.answer.synth_vllm_localhost",
        "builtin.embedder.vllm_localhost",
        "builtin.vlm.vllm_localhost"
      ]
    },
    "settings": {
      "__golden_profile": {
        "determinism": {
          "blocked_env_overrides": [
            "AUTOCAPTURE_DISABLE_REQUIRED_PLUGIN_GATE",
            "AUTOCAPTURE_IDLE_VLM_EXTRACT",
            "AUTOCAPTURE_QWEN_MAX_ROIS",
            "AUTOCAPTURE_QWEN_MODEL",
            "AUTOCAPTURE_QWEN_ROI_MAX_NEW_TOKENS",
            "AUTOCAPTURE_QWEN_ROI_MAX_SIDE",
            "AUTOCAPTURE_QWEN_THUMB_MAX_NEW_TOKENS",
            "AUTOCAPTURE_VLM_MAX_ROIS",
            "AUTOCAPTURE_VLM_MAX_TOKENS",
            "AUTOCAPTURE_VLM_ROI_MAX_SIDE",
            "AUTOCAPTURE_VLM_ROI_MAX_TOKENS",
            "AUTOCAPTURE_VLM_THUMB_MAX_PX",
            "AUTOCAPTURE_VLM_THUMB_MAX_TOKENS",
            "AUTOCAPTURE_VLM_TIMEOUT_S"
          ],
          "lang": "C.UTF-8",
          "pythonhashseed": "0",
          "repro_runs": 3,
          "timezone": "UTC"
        },
        "enable_synthesizer": true,
        "profile_id": "golden_full_v1",
        "required_plugins": [
          "builtin.anchor.basic",
          "builtin.storage.sqlcipher",
          "builtin.journal.basic",
          "builtin.ledger.basic",
          "builtin.ocr.basic",
          "builtin.ocr.nemotron_torch",
          "builtin.ocr.rapid",
          "builtin.processing.sst.pipeline",
          "builtin.processing.sst.ui_vlm",
          "builtin.vlm.vllm_localhost",
          "builtin.embedder.vllm_localhost",
          "builtin.index.colbert_hash",
          "builtin.reranker.colbert_hash",
          "builtin.retrieval.basic",
          "builtin.reranker.basic",
          "builtin.state.evidence.compiler",
          "builtin.state.jepa_like",
          "builtin.state.retrieval",
          "builtin.state.vector.hnsw",
          "builtin.state.vector.sqlite_ts",
          "builtin.observation.graph",
          "builtin.prompt.bundle.default",
          "builtin.answer.synth_vllm_localhost"
        ]
      },
      "builtin.answer.synth_vllm_localhost": {
        "base_url": "http://127.0.0.1:8000/v1",
        "max_evidence_chars": 3600,
        "max_tokens": 768,
        "system_prompt_path": "promptops/prompts/answer_synth_system.txt",
        "query_context_pre_path": "promptops/prompts/answer_synth_query_pre.txt",
        "query_context_post_path": "promptops/prompts/answer_synth_query_post.txt",
        "promptops": {
          "enabled": true,
          "metrics": {
            "enabled": true
          },
          "mode": "auto_apply",
          "model_strategy": "model_contract",
          "persist_prompts": false,
          "review": {
            "auto_approve": true,
            "allow_empty_examples": false,
            "base_url": "http://127.0.0.1:8000/v1",
            "enabled": true,
            "on_failure_only": true,
            "preflight_auto_recover": true,
            "preflight_completion_timeout_s": 20.0,
            "preflight_retries": 1,
            "preflight_require_completion": true,
            "require_preflight": true,
            "persist_prompts": true,
            "timeout_s": 20.0
          }
        },
        "timeout_s": 45.0
      },
      "builtin.ocr.rapid": {
        "models": {
          "rapidocr": {
            "max_image_side": 3072,
            "max_threads": 1
          }
        }
      },
      "builtin.ocr.nemotron_torch": {
        "base_url": "http://127.0.0.1:8000/v1",
        "max_tokens": 1024,
        "timeout_s": 45.0
      },
      "builtin.embedder.vllm_localhost": {
        "base_url": "http://127.0.0.1:8001",
        "model": "BAAI/bge-small-en-v1.5",
        "strict_remote_only": true,
        "timeout_s": 20.0
      },
      "builtin.vlm.vllm_localhost": {
        "base_url": "http://127.0.0.1:8000/v1",
        "model": "internvl3_5_8b",
        "n": 1,
        "max_rois": 7,
        "grid_sections": 6,
        "grid_enforced": true,
        "max_tokens": 640,
        "max_retries": 2,
        "timeout_s": 25.0,
        "roi_max_side": 2048,
        "roi_max_tokens": 1024,
        "seed": 0,
        "temperature": 0.0,
        "top_p": 1.0,
        "thumb_max_px": 1536,
        "thumb_max_tokens": 640,
        "factpack_enabled": true,
        "factpack_max_tokens": 1280,
        "topic_factpack_enabled": true,
        "topic_factpack_max_tokens": 768,
        "topic_factpack_min_topics": 2,
        "promptops": {
          "enabled": true,
          "metrics": {
            "enabled": true
          },
          "mode": "auto_apply",
          "model_strategy": "model_contract",
          "persist_prompts": false,
          "review": {
            "auto_approve": true,
            "allow_empty_examples": false,
            "base_url": "http://127.0.0.1:8000/v1",
            "enabled": true,
            "on_failure_only": true,
            "preflight_auto_recover": true,
            "preflight_completion_timeout_s": 20.0,
            "preflight_retries": 1,
            "preflight_require_completion": true,
            "require_preflight": true,
            "persist_prompts": true,
            "timeout_s": 20.0
          }
        },
        "two_pass_enabled": true
      }
    }
  },
  "promptops": {
    "persist_query_prompts": true,
    "review": {
      "auto_approve": true,
      "allow_empty_examples": false,
      "preflight_auto_recover": true,
      "preflight_completion_timeout_s": 20.0,
      "preflight_retries": 1,
      "preflight_require_completion": true,
      "require_preflight": true
    }
  },
  "processing": {
    "idle": {
      "extractors": {
        "ocr": false,
        "vlm": false
      }
    },
    "on_query": {
      "allow_decode_extract": false,
      "extractors": {
        "ocr": false,
        "vlm": false
      },
      "require_idle": true
    },
    "sst": {
      "ui_parse": {
        "enabled": true,
        "fallback_detector": false,
        "max_providers": 1,
        "mode": "vlm_json"
      },
      "ui_vlm": {
        "enabled": true,
        "max_providers": 1,
        "prefer_live_vlm": true,
        "use_cached_tokens": false
      }
    },
    "state_layer": {
      "features": {
        "anomaly_enabled": true,
        "index_enabled": true,
        "training_enabled": true,
        "workflow_enabled": true
      }
    }
  },
  "storage": {
    "no_deletion_mode": false,
    "retention": {
      "evidence": "6d",
      "images_only": true,
      "interval_s": 86400,
      "max_delete_per_run": 5000,
      "processed_only": true,
      "record_batches": true
    }
  }
}
