    {
      "kind": "answer.builder",
      "id": "default",
      "path": "plugin.py",
      "callable": "create_plugin"
    }
  ],
  "permissions": {
    "filesystem": "read",
    "gpu": false,
    "raw_input": false,
    "network": false
