    "requires_kernel": ">=0.1.0",
    "requires_schema_versions": [1]
  },
  "depends_on": ["builtin.citation.basic"],
  "required_capabilities": [
    "citation.validator"
  ],
  "hash_lock": {
    "manifest_sha256": "",
    "artifact_sha256": ""
  }
}
