{
  "compat": {
    "requires_kernel": ">=0.1.0",
    "requires_schema_versions": [
      1
    ]
  },
  "depends_on": [
    "builtin.journal.basic",
    "builtin.ledger.basic",
    "builtin.anchor.basic",
    "builtin.backpressure.basic"
  ],
  "enabled": true,
  "entrypoints": [
    {
      "callable": "create_plugin",
      "id": "default",
      "kind": "capture.source",
      "path": "plugin.py"
    }
  ],
  "hash_lock": {
    "artifact_sha256": "",
    "manifest_sha256": ""
  },
  "permissions": {
    "filesystem": "readwrite",
    "gpu": true,
    "network": false,
    "raw_input": true
  },
  "plugin_id": "builtin.capture.windows",
  "required_capabilities": [
    "capture.backpressure",
    "event.builder",
    "observability.logger",
    "storage.media",
    "storage.metadata"
  ],
  "version": "0.1.0"
}