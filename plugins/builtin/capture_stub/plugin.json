{
  "plugin_id": "builtin.capture.basic",
  "version": "0.1.0",
  "enabled": false,
  "entrypoints": [
    {
      "kind": "capture.source",
      "id": "default",
      "path": "plugin.py",
      "callable": "create_plugin"
    }
  ],
  "permissions": {
    "filesystem": "readwrite",
    "gpu": false,
    "raw_input": false,
    "network": false
  },
  "compat": {
    "requires_kernel": ">=0.1.0",
    "requires_schema_versions": [
      1
    ]
  },
  "depends_on": [
    "builtin.journal.basic",
    "builtin.ledger.basic",
    "builtin.anchor.basic",
    "builtin.backpressure.basic"
  ],
  "required_capabilities": [
    "storage.media",
    "storage.metadata",
    "event.builder",
    "capture.backpressure",
    "observability.logger",
    "window.metadata",
    "tracking.input"
  ],
  "hash_lock": {
    "manifest_sha256": "",
    "artifact_sha256": ""
  },
  "replaces": [
    "builtin.capture.stub"
  ]
}
