{
  "compat": {
    "requires_kernel": ">=0.1.0",
    "requires_schema_versions": [
      1
    ]
  },
  "depends_on": [
    "builtin.privacy.egress_sanitizer"
  ],
  "enabled": true,
  "entrypoints": [
    {
      "callable": "create_plugin",
      "id": "default",
      "kind": "egress.gateway",
      "path": "plugin.py"
    }
  ],
  "hash_lock": {
    "artifact_sha256": "",
    "manifest_sha256": ""
  },
  "permissions": {
    "filesystem": "readwrite",
    "gpu": false,
    "network": true,
    "raw_input": false
  },
  "plugin_id": "builtin.egress.gateway",
  "required_capabilities": [
    "privacy.egress_sanitizer",
    "event.builder",
    "egress.approval_store"
  ],
  "settings_paths": [
    "gateway",
    "plugins.permissions",
    "plugins.safe_mode",
    "privacy",
    "storage"
  ],
  "version": "0.1.0"
}
