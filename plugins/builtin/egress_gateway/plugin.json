{
  "plugin_id": "builtin.egress.gateway",
  "version": "0.1.0",
  "enabled": true,
  "entrypoints": [
    {
      "kind": "egress.gateway",
      "id": "default",
      "path": "plugin.py",
      "callable": "create_plugin"
    }
  ],
  "permissions": {
    "filesystem": "readwrite",
    "gpu": false,
    "raw_input": false,
    "network": true
  },
  "compat": {
    "requires_kernel": ">=0.1.0",
    "requires_schema_versions": [1]
  },
  "depends_on": ["builtin.privacy.egress_sanitizer"],
  "required_capabilities": [
    "privacy.egress_sanitizer"
  ],
  "hash_lock": {
    "manifest_sha256": "",
    "artifact_sha256": ""
  }
}
