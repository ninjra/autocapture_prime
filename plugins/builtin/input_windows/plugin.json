{
  "plugin_id": "builtin.tracking.input.windows",
  "version": "0.1.0",
  "enabled": true,
  "entrypoints": [
    {
      "kind": "tracking.input",
      "id": "default",
      "path": "plugin.py",
      "callable": "create_plugin"
    }
  ],
  "permissions": {
    "filesystem": "readwrite",
    "gpu": false,
    "raw_input": true,
    "network": false
  },
  "compat": {
    "requires_kernel": ">=0.1.0",
    "requires_schema_versions": [1]
  },
  "depends_on": ["builtin.journal.basic"],
  "hash_lock": {
    "manifest_sha256": "",
    "artifact_sha256": ""
  }
}
